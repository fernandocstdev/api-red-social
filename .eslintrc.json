{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-tabs": "off",
        "indent": [
            "error",
            "tab"
        ],
        "space-before-function-paren": "off"
    }
}